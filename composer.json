{
    "name": "1tomany/php-ai",
    "description": "A single, unified, framework-independent library for integration with many popular AI platforms and large language models",
    "license": "MIT",
    "type": "library",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "keywords": [
        "ai",
        "ai client",
        "chatgpt",
        "chat gpt",
        "claude",
        "gemini",
        "gpt",
        "openai",
        "open ai"
    ],
    "require": {
        "php": ">=8.4",
        "ext-fileinfo": "*",
        "fakerphp/faker": "^1.24",
        "phpdocumentor/reflection-docblock": "^5.6",
        "symfony/http-client": "^7.2|^8.0",
        "symfony/property-access": "^7.2|^8.0",
        "symfony/property-info": "^7.2|^8.0",
        "symfony/serializer": "^7.2|^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.93",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^12.5"
    },
    "autoload": {
        "psr-4": {
            "OneToMany\\AI\\": "src/"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "OneToMany\\AI\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "fix": "./vendor/bin/php-cs-fixer fix",
        "stan": "./vendor/bin/phpstan",
        "test": "./vendor/bin/phpunit"
    },
    "authors": [
        {
            "name": "Vic Cherubini",
            "email": "vcherubini@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true
    }
}
